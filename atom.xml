<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Mister Bleigh</title>
 <link href="http://captproton.github.com/atom.xml" rel="self"/>
 <link href="http://www.captproton.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.captproton.com/</id>
 <author>
   <name>Carl Tanner</name>
   <email>captproton@captproton.com</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.captproton.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://captproton.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
